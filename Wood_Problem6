from NumberTests import squareSum
from NumberTests import sumSquare




def main():
    num = 100
    # num = int(input("Number: "))
    sumSq = sumSquare(num)
    sqSum = squareSum(num)
    print("The sum of the squares of",num,"is:",sumSq)
    print("The square of the sum of",num,"is:",sqSum)
    print("The difference between the square of the sum and the sum of the squares of",num,"is:",sqSum-sumSq)

if __name__ == '__main__':
  main()