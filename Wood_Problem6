# Wood_Problem6.py
# Sum Square Difference
# Brennan Wood 
# 3/8/25

def sumSquare(num):
    squares = []
    for i in range(1, num + 1):
        square = i * i
        squares.append(square)
    return sum(squares)

def squareSum(num):
    sum = 0
    for i in range(1, num + 1):
        sum = sum + i
    return sum ** 2




def main():
    num = 100
    # num = int(input("Number: "))
    sumSq = sumSquare(num)
    sqSum = squareSum(num)
    print("The sum of the squares of",num,"is:",sumSq)
    print("The square of the sum of",num,"is:",sqSum)
    print("The difference between the square of the sum and the sum of the squares of",num,"is:",sqSum-sumSq)

if __name__ == '__main__':
  main()